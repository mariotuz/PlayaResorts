<?xml version="1.0" encoding="utf-8" ?>
<querys>
  <query id="InsertUser" titulo="Guarda Usuario">
    <queryString>exec sp_guarda_usuario</queryString>
    <parameter>UserName</parameter>
    <parameter>UserLastname</parameter>
    <parameter>UserEmail</parameter>
    <parameter>UserEmailHyatt</parameter>
    <parameter>UserNumberEmploye</parameter>
    <parameter>UserLogin</parameter>
    <parameter>UserPassword</parameter>
    <parameter>userprofile</parameter>
    <parameter>userchannel</parameter>
    <parameter>UserMaxDiscount</parameter>
    <parameter>usertypeuser</parameter>
    <parameter>userproperties</parameter>
    <parameter codification="Base64">xml</parameter>
  </query>
  <query id="UpdateUser" titulo="Actualiza Usuario">
    <queryString>exec sp_actualiza_usuario</queryString>
    <parameter>Id_User</parameter>
    <parameter>UserName</parameter>
    <parameter>UserLastname</parameter>
    <parameter>UserEmail</parameter>
    <parameter>UserEmailHyatt</parameter>
    <parameter>UserNumberEmploye</parameter>
    <parameter>UserLogin</parameter>
    <parameter>UserPassword</parameter>
    <parameter>userprofile</parameter>
    <parameter>userchannel</parameter>
    <parameter>UserMaxDiscount</parameter>
    <parameter>usertypeuser</parameter>
    <parameter>userproperties</parameter>
    <parameter codification="Base64">xml</parameter>
  </query>
  <query id="GetDataUser" titulo="Obtiene datos de usuario">
    <queryString>exec sp_obtiene_datos_usuario</queryString>
    <parameter>id_user</parameter>
    <parameter>solo_permisos</parameter>
    <parameter>id_secciones</parameter>
  </query>
  <query id="GetGroupsRate" titulo="Obtiene Rates de Grupos">
    <queryString>EXEC SP_GROUPS_RATES_LIST</queryString>
    <parameter>id</parameter>
  </query>
  <query id="GetGroupListoLD" titulo="Obtiene lista de grupos">
    <queryString>EXEC SP_GROUPS_LIST</queryString>
    <parameter>ID_GROUP</parameter>
    <parameter>FILTERS</parameter>
    <parameter>ORDER</parameter>
  </query>
  <query id="GetGroupList" titulo="Obtiene lista de grupos">
    <queryString>SP_GROUPS_LIST_TEST</queryString>
    <parameter dbname="User">User</parameter>
    <parameter dbname="Id_Group">Id_Group</parameter>
    <parameter dbname="Hoteles">Hoteles</parameter>
    <parameter dbname="BookStatus">BookStatus</parameter>
    <parameter dbname="HotelStatus">HotelStatus</parameter>
    <parameter dbname="PayStatus">PayStatus</parameter>
    <parameter dbname="Coordinators">Coordinators</parameter>
    <parameter dbname="GroupTypes">GroupTypes</parameter>
    <parameter dbname="Markets">Markets</parameter>
    <parameter dbname="AgencyTypes">AgencyTypes</parameter>
    <parameter dbname="DateFilter">DateFilter</parameter>
    <parameter dbParam="6,1" dbname="FechaIni">FechaIni</parameter>
    <parameter dbParam="6,1" dbname="FechaFin">FechaFin</parameter>
    <parameter dbname="OrderType1">OrderType1</parameter>
    <parameter dbname="OrderTypeD1">OrderTypeD1</parameter>
    <parameter dbname="OrderType2">OrderType2</parameter>
    <parameter dbname="OrderTypeD2">OrderTypeD2</parameter>
    <parameter dbParam="4,2" dbname="TotalRevenue">TotalRevenue</parameter>
  </query>
  <query id="InsertProfile" titulo="Inserta perfiles">
    <queryString>SP_INSERT_PROFILE</queryString>
    <parameter>Id_User</parameter>
    <parameter>ProfileName</parameter>
    <parameter codification="Base64">xml</parameter>
  </query>
  <query id="GetDataProfile" titulo="Obtiene datos de perfiles">
    <queryString>exec sp_obtiene_datos_perfiles</queryString>
    <parameter>id_profile</parameter>
    <parameter>solo_permisos</parameter>
    <parameter>id_seccion</parameter>
  </query>
  <query id="UpdateProfile" titulo="Actualiza Perfil">
    <queryString>sp_actualiza_Perfil</queryString>
    <parameter>Id_User</parameter>
    <parameter>Id_Profile</parameter>
    <parameter>ProfileName</parameter>
    <parameter codification="Base64">xml</parameter>
  </query>
  <query id="BreakData" titulo="Datos break down">
    <queryString>sp_Datos_BreakDown</queryString>
    <parameter>Id_Group</parameter>
    <parameter>Group_By</parameter>
  </query>
  <query id="InsertBreak" titulo="Inserta break down">
    <queryString>sp_inserta_breakdown</queryString>
    <parameter>breaknumroom</parameter>
    <parameter dbname="code_room">selectypesuitebreak</parameter>
    <parameter dbname="checkin">datetimepickerdate1text</parameter>
    <parameter dbname="checkout">datetimepickerdate2text</parameter>
    <parameter dbname="adults">breakadults</parameter>
    <parameter dbname="teens">breakteens</parameter>
    <parameter dbname="childs">breakchilds</parameter>
    <parameter dbname="infants">breakinfants</parameter>
    <parameter dbname="id_group">grupoidbreakdown</parameter>
    <parameter dbname="guestname">editbreakname</parameter>
    <parameter dbname="guestlastname">editbreaklastname</parameter>
    <parameter dbname="special_notes">notes</parameter>
    <parameter>selectgroupby</parameter>
    <parameter>Cve_Hotel</parameter>
  </query>
  <query id="InsertCompany" titulo="Inserta compañia">
    <queryString>sp_inserta_compania</queryString>
    <parameter dbname="Name">CompanieName</parameter>
    <parameter dbname="Key">CompanieKey</parameter>
    <parameter dbname="IATA">CompanieIATA</parameter>
    <parameter dbname="Postal_Code">CompanieZipCode</parameter>
    <parameter dbname="Country">companycountry</parameter>
    <parameter dbname="Sate">companystate</parameter>
    <parameter>City</parameter>
    <parameter>Address</parameter>
    <parameter>User</parameter>
  </query>
</querys>